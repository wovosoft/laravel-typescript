{
    "name": "wovosoft/laravel-typescript",
    "description": "Transforms Laravel Models to Typescript Interfaces/Types",
    "license": "MIT",
    "authors": [
        {
            "name": "Narayan Adhikary",
            "email": "wovosoft@gmail.com",
            "homepage": "https://wovosoft.com"
        }
    ],
    "homepage": "https://github.com/wovosoft/laravel-typescript",
    "keywords": [
        "Laravel",
        "Typescript Transformer",
        "Typescript Interfaces",
        "Typescript Types",
        "php to typescript"
    ],
    "require": {
        "composer/class-map-generator": "*"
    },
    "autoload": {
        "psr-4": {
            "Wovosoft\\LaravelTypescript\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wovosoft\\LaravelTypescript\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wovosoft\\LaravelTypescript\\LaravelTypescriptServiceProvider"
            ],
            "aliases": {
                "LaravelTypescript": "Wovosoft\\LaravelTypescript\\Facades\\LaravelTypescript"
            }
        }
    }
}
